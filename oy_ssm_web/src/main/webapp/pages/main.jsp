        #notice{
            /*resize: none;*/
            background: #bfc099;
            border-radius:15px;
            padding: 25px;
            margin: 50px;
            border: 0px;
            width: 300px;
            height: 200px;
            float: right;

            -moz-box-shadow:10px 10px 7px rgba(0,0,0,7);        /*阴影的颜色和大小*/
