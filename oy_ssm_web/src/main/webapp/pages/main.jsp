            -o-transform:scale(1.25);
            position:relative;                                  /*绝对定位*/
            z-index:5;

        }

        #notice h3{
            margin-top: -15px;
            text-align: center;
            color: #311008;
            font-weight: bolder;
        }

        #notice p{
            color: #260903;
            font-style: italic;
            font-family:新宋体;
        }

        #notice p:hover{
            color: #a8160e;
            font-family:"Adobe 宋体 Std L";
        }


